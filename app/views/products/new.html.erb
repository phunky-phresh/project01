<h1>Add Product</h1>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :model %>
  <%= f.text_field :model %>
  <%= f.label :image %>
  <%= f.url_field :image %>


  <%= f.submit %>
<% end %>
